{
  "wilson-cowan": {
    "name": "Wilson-Cowan",
    "description": "Neural population dynamics model",
    "params": {
      "w_ee": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ee}", "description": "Excitatory to excitatory weight" },
      "w_ei": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ei}", "description": "Inhibitory to excitatory weight" },
      "w_ie": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ie}", "description": "Excitatory to inhibitory weight" },
      "w_ii": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ii}", "description": "Inhibitory to inhibitory weight" },
      "theta_e": { "min": 0, "max": 10, "value": 4, "step": 0.1, "latex": "\\theta_e", "description": "Excitatory threshold" },
      "theta_i": { "min": 0, "max": 10, "value": 3.7, "step": 0.1, "latex": "\\theta_i", "description": "Inhibitory threshold" },
      "P": { "min": 0, "max": 5, "value": 1.25, "step": 0.05, "latex": "P", "description": "External input to excitatory" },
      "Q": { "min": 0, "max": 5, "value": 0, "step": 0.05, "latex": "Q", "description": "External input to inhibitory" },
      "tau_e": { "min": 0.1, "max": 20, "value": 8, "step": 0.1, "latex": "\\tau_e", "description": "Excitatory time constant" },
      "tau_i": { "min": 0.1, "max": 20, "value": 8, "step": 0.1, "latex": "\\tau_i", "description": "Inhibitory time constant" }
    },
    "initial_conditions": [0.1, 0.1],
    "plots": [
      {
        "indices": [0, 1],
        "axes": ["E", "I"],
        "axis_ranges": {
          "x": { "min": 0, "max": 1 },
          "y": { "min": 0, "max": 1 }
        }
      }
    ],
    "formula": "\\begin{align*} \\tau_e \\frac{dE}{dt} &= -E + (1 - E) S(w_{ee}E - w_{ei}I - \\theta_e + P) \\\\ \\tau_i \\frac{dI}{dt} &= -I + (1 - I) S(w_{ie}E - w_{ii}I - \\theta_i + Q) \\end{align*}",
    "ode_type": "wilson_cowan"
  },
  "damped-harmonic": {
    "name": "Damped Harmonic Oscillator",
    "description": "Mass-spring-damper system",
    "params": {
      "omega": { "min": 0.1, "max": 5, "value": 1, "step": 0.1, "latex": "\\omega", "description": "Natural frequency" },
      "gamma": { "min": 0, "max": 2, "value": 0.1, "step": 0.01, "latex": "\\gamma", "description": "Damping coefficient" },
      "A": { "min": 0, "max": 5, "value": 0, "step": 0.1, "latex": "A", "description": "External forcing amplitude" },
      "omega_f": { "min": 0.1, "max": 5, "value": 1, "step": 0.1, "latex": "\\omega_f", "description": "Forcing frequency" }
    },
    "initial_conditions": [1, 0],
    "plots": [
      {
        "indices": [0, 1],
        "axes": ["x", "v"],
        "axis_ranges": {
          "x": { "min": -2, "max": 2 },
          "y": { "min": -3, "max": 3 }
        }
      }
    ],
    "formula": "\\begin{align*} \\frac{dx}{dt} &= v \\\\ \\frac{dv}{dt} &= -\\omega^2 x - 2\\gamma v + A \\cos(\\omega_f t) \\end{align*}",
    "ode_type": "damped_harmonic"
  },
  "coupled-wilson-cowan": {
    "name": "Coupled Wilson-Cowan Oscillators",
    "description": "Two interconnected neural population oscillators",
    "params": {
      "w_ee1": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ee}^{(1)}", "description": "Excitatory to excitatory weight (oscillator 1)" },
      "w_ei1": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ei}^{(1)}", "description": "Inhibitory to excitatory weight (oscillator 1)" },
      "w_ie1": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ie}^{(1)}", "description": "Excitatory to inhibitory weight (oscillator 1)" },
      "w_ii1": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ii}^{(1)}", "description": "Inhibitory to inhibitory weight (oscillator 1)" },
      "w_ee2": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ee}^{(2)}", "description": "Excitatory to excitatory weight (oscillator 2)" },
      "w_ei2": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ei}^{(2)}", "description": "Inhibitory to excitatory weight (oscillator 2)" },
      "w_ie2": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ie}^{(2)}", "description": "Excitatory to inhibitory weight (oscillator 2)" },
  "w_ii2": { "min": 0, "max": 20, "value": 10, "step": 0.1, "latex": "w_{ii}^{(2)}", "description": "Inhibitory to inhibitory weight (oscillator 2)" },
  "w_ce12": { "min": 0, "max": 10, "value": 2, "step": 0.1, "latex": "w_{ce}^{1 \\rightarrow 2}", "description": "Excitatory cross-coupling from oscillator 1 to excitatory input of oscillator 2" },
  "w_ci12": { "min": 0, "max": 10, "value": 2, "step": 0.1, "latex": "w_{ci}^{1 \\rightarrow 2}", "description": "Excitatory cross-coupling from oscillator 1 to inhibitory input of oscillator 2" },
  "w_ce21": { "min": 0, "max": 10, "value": 2, "step": 0.1, "latex": "w_{ce}^{2 \\rightarrow 1}", "description": "Excitatory cross-coupling from oscillator 2 to excitatory input of oscillator 1" },
  "w_ci21": { "min": 0, "max": 10, "value": 2, "step": 0.1, "latex": "w_{ci}^{2 \\rightarrow 1}", "description": "Excitatory cross-coupling from oscillator 2 to inhibitory input of oscillator 1" },
      "theta_e1": { "min": 0, "max": 10, "value": 4, "step": 0.1, "latex": "\\theta_e^{(1)}", "description": "Excitatory threshold (oscillator 1)" },
      "theta_i1": { "min": 0, "max": 10, "value": 3.7, "step": 0.1, "latex": "\\theta_i^{(1)}", "description": "Inhibitory threshold (oscillator 1)" },
      "theta_e2": { "min": 0, "max": 10, "value": 4, "step": 0.1, "latex": "\\theta_e^{(2)}", "description": "Excitatory threshold (oscillator 2)" },
      "theta_i2": { "min": 0, "max": 10, "value": 3.7, "step": 0.1, "latex": "\\theta_i^{(2)}", "description": "Inhibitory threshold (oscillator 2)" },
      "P1": { "min": 0, "max": 5, "value": 1.25, "step": 0.05, "latex": "P^{(1)}", "description": "External input to excitatory (oscillator 1)" },
      "Q1": { "min": 0, "max": 5, "value": 0, "step": 0.05, "latex": "Q^{(1)}", "description": "External input to inhibitory (oscillator 1)" },
      "P2": { "min": 0, "max": 5, "value": 1.25, "step": 0.05, "latex": "P^{(2)}", "description": "External input to excitatory (oscillator 2)" },
      "Q2": { "min": 0, "max": 5, "value": 0, "step": 0.05, "latex": "Q^{(2)}", "description": "External input to inhibitory (oscillator 2)" },
      "tau_e1": { "min": 0.1, "max": 20, "value": 8, "step": 0.1, "latex": "\\tau_e^{(1)}", "description": "Excitatory time constant (oscillator 1)" },
      "tau_i1": { "min": 0.1, "max": 20, "value": 8, "step": 0.1, "latex": "\\tau_i^{(1)}", "description": "Inhibitory time constant (oscillator 1)" },
      "tau_e2": { "min": 0.1, "max": 20, "value": 8, "step": 0.1, "latex": "\\tau_e^{(2)}", "description": "Excitatory time constant (oscillator 2)" },
      "tau_i2": { "min": 0.1, "max": 20, "value": 8, "step": 0.1, "latex": "\\tau_i^{(2)}", "description": "Inhibitory time constant (oscillator 2)" }
    },
    "initial_conditions": [0.1, 0.1, 0.2, 0.2],
    "requires_all_phase_groups": true,
    "phase_groups": [
      {
        "name": "Oscillator 1",
        "indices": [0, 1],
        "axes": ["E₁", "I₁"],
        "axis_ranges": {
          "x": { "min": 0, "max": 1 },
          "y": { "min": 0, "max": 1 }
        }
      },
      {
        "name": "Oscillator 2",
        "indices": [2, 3],
        "axes": ["E₂", "I₂"],
        "axis_ranges": {
          "x": { "min": 0, "max": 1 },
          "y": { "min": 0, "max": 1 }
        }
      }
    ],
    "plots": [
      {
        "indices": [0, 1],
        "axes": ["E₁", "I₁"],
        "axis_ranges": {
          "x": { "min": 0, "max": 1 },
          "y": { "min": 0, "max": 1 }
        }
      },
      {
        "indices": [2, 3],
        "axes": ["E₂", "I₂"],
        "axis_ranges": {
          "x": { "min": 0, "max": 1 },
          "y": { "min": 0, "max": 1 }
        }
      }
    ],
  "formula": "\\begin{align*} \\tau_e^{(1)} \\frac{dE_1}{dt} &= -E_1 + (1 - E_1) S(w_{ee}^{(1)}E_1 - w_{ei}^{(1)}I_1 + w_{ce}^{2 \\rightarrow 1}E_2 - \\theta_e^{(1)} + P^{(1)}) \\\\ \\tau_i^{(1)} \\frac{dI_1}{dt} &= -I_1 + (1 - I_1) S(w_{ie}^{(1)}E_1 - w_{ii}^{(1)}I_1 + w_{ci}^{2 \\rightarrow 1}E_2 - \\theta_i^{(1)} + Q^{(1)}) \\\\ \\tau_e^{(2)} \\frac{dE_2}{dt} &= -E_2 + (1 - E_2) S(w_{ee}^{(2)}E_2 - w_{ei}^{(2)}I_2 + w_{ce}^{1 \\rightarrow 2}E_1 - \\theta_e^{(2)} + P^{(2)}) \\\\ \\tau_i^{(2)} \\frac{dI_2}{dt} &= -I_2 + (1 - I_2) S(w_{ie}^{(2)}E_2 - w_{ii}^{(2)}I_2 + w_{ci}^{1 \\rightarrow 2}E_1 - \\theta_i^{(2)} + Q^{(2)}) \\end{align*}",
    "ode_type": "coupled_wilson_cowan"
  }
}
